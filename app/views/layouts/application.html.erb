<!DOCTYPE html>
<html>
  <head>
    <title>CurvaProto2</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'bootstrap', 'data-turbolinks-track': 'reload' %>
    <%= yield :head %>
    <%= render 'layouts/shim' %>
  </head>

  <body data-env="<%= Rails.env %>">
    <div class="ak-page">
      <%= render 'layouts/header' %>

        <%= yield %>

        <%= render 'layouts/mobile_menu' %>      
        <%= render 'layouts/social' %>
        <%= render 'layouts/footer' %>
      
    </div>
  </body>
</html>
